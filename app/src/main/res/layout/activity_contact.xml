<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.water.eldowas.ui.activity.TambuaActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:background="@color/twitter_blue"
        android:id="@+id/linyhll"
        android:layout_height="wrap_content">

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:id="@+id/about_back"
            android:background="@color/twitter_blue"
            android:src="@drawable/ic_action_back"/>

        <TextView
            android:layout_width="match_parent"
            android:text="Contacts and FAQs"
            android:textAlignment="center"
            android:textSize="20dp"
            android:layout_marginTop="10dp"
            android:textStyle="bold"
            android:id="@+id/tambua_hd"
            android:layout_height="wrap_content" />
    </LinearLayout>
    <WebView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:id="@+id/webViewz"
        android:layout_below="@+id/linyhll"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        />






</RelativeLayout>
